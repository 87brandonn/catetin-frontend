{
  "expo": {
    "name": "catetin-frontend",
    "slug": "catetin-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier" : "com.brandonpardede.catetinapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package" : "com.brandonpardede.catetinapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme" : "com.brandonpardede.catetinapp"
  }
}
