{
  "expo": {
    "name": "Catetin",
    "slug": "catetin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2461FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brandonpardede.catetinapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.brandonpardede.catetinapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.brandonpardede.catetinapp",
    "facebookScheme" : "fb709036773596885",
    "facebookAppId" : "709036773596885",
    "facebookDisplayName" : "Brandon Pardede",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-facebook",
        {
          "userTrackingPermission": false
        }
      ]
    ]
  }
}
