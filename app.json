{
  "expo": {
    "name": "Catetin",
    "slug": "catetin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2461FF"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "catetin",
            "project": "react-native",
            "authToken": "29a37f8ecba84ed8a93ea673a11270ffeca0dcbf58f848b2ad13734f3288ad06"
          }
        }
      ]
    },
    "backgroundColor": "#2461FF",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brandonpardede.catetinapp"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.brandonpardede.catetinapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "com.brandonpardede.catetinapp",
    "facebookScheme": "fb709036773596885",
    "facebookAppId": "709036773596885",
    "facebookDisplayName": "Brandon Pardede",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-facebook",
        {
          "userTrackingPermission": false
        }
      ],
      ["sentry-expo"]
    ]
  }
}
